{
  "name": "nuxt-directus-sdk",
  "type": "module",
  "version": "0.0.16",
  "description": "A nuxt module that uses the directus SDK",
  "author": "Matthew Rollinson <matt@rolley.io>",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "docs:dev": "bun run dev:prepare && vitepress dev docs",
    "docs:build": "bun run dev:prepare && vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@directus/sdk": "^13.0.0",
    "@directus/types": "^11.0.1",
    "@nuxt/image": "^1.0.0",
    "@nuxt/kit": "^3.7.1",
    "change-case": "^4.1.2",
    "defu": "^6.1.1",
    "ufo": "^1.3.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@nuxt/devtools": "latest",
    "@nuxt/module-builder": "^0.5.1",
    "@nuxt/schema": "^3.7.1",
    "@nuxt/test-utils": "^3.7.1",
    "@types/node": "^18.17.14",
    "changelogen": "^0.5.5",
    "nuxt": "^3.7.1",
    "vitepress": "^1.0.0-rc.24",
    "vitest": "^0.34.2"
  },
  "unbuild": {
    "failOnWarn": false
  }
}
